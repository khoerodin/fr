{% extends 'layout.twig' %}

{% block js %}
    <script src="/assets/scripts/Adv/Customers.js?{{ microtime }}"></script>
    <script src="/assets/scripts/Adv/Specifications.js?{{ microtime }}"></script>
    <script src="/assets/scripts/Adv/SpecificationDetails.js?{{ microtime }}"></script>
    <script src="/assets/scripts/Adv/Prices.js?{{ microtime }}"></script>
    <script src="/assets/scripts/Adv/AccountExecutives.js?{{ microtime }}"></script>
    <script src="/assets/scripts/Adv/Media.js?{{ microtime }}"></script>
    <script src="/assets/scripts/Admin/Cities.js?{{ microtime }}"></script>
    <script src="/assets/scripts/Adv/OrderForm.js?{{ microtime }}"></script>
    <script src="/assets/scripts/Adv/Orders.js?{{ microtime }}"></script>
    <script src="/assets/scripts/Adv/PublishAds.js?{{ microtime }}"></script>
    <script src="/assets/scripts/Adv/OrderDetail.js?{{ microtime }}"></script>
    <script>
        // Order From
        var orderFormElm = document.querySelector('[name="orderFrom"]');
        orderFormElm.innerHTML = '<option value="/api/representatives/{{ order.orderFrom.id }}">{{ order.orderFrom.name }}</option>';
        Bisnis.Util.Event.bind('change', '[name="orderFrom"]');
        // End Order From

        // Tanggal Booking & Tanggal Faktur
        Bisnis.Util.DatePicker.render('#dtBookedAt', '{{ order.bookedAt }}');
        Bisnis.Util.DatePicker.render('#dtInvoicedAt', '{{ order.invoicedAt }}');
        // End Tanggal Booking & Tanggal Faktur

        // Edisi Terbit
        Bisnis.Util.Event.bind('click','#edisiTerbitButton', function () {
            loadPublishAds(1, '{{ order.id }}');
            Bisnis.Util.Dialog.showModal('#edisiTerbitModal');
        });

        var loadPublishAds = function (pageNum, orderId) {
            pageNum = (!pageNum || 'null' === pageNum ) ? 1 : pageNum;
            Bisnis.Util.Storage.store('DT_ORDER_PUBLISH_ADS_CURRENT_PAGE', pageNum);
            Bisnis.Adv.PublishAds.fetchAll([{page: pageNum},{'order.id': orderId}, {order: {publishDate: 'ASC'}}],
                function (dataResponse) {
                    var memberData = dataResponse['hydra:member'];
                    var viewData = dataResponse['hydra:view'];

                    if ('undefined' !== typeof viewData['hydra:last']) {
                        var currentPage = Bisnis.Util.Url.getQueryParam('page', viewData['@id']);
                        Bisnis.Util.Grid.createPagination('#publishAdsPagination', Bisnis.Util.Url.getQueryParam('page', viewData['hydra:last']), currentPage);
                    }

                    if (memberData.length > 0) {
                        var records = [];
                        Bisnis.each(function (idx, memberData) {
                            records.push([
                                { value: memberData.publishDate, format: function (date) {
                                    return moment(date).format('DD/MM/YYYY');
                                } },
                            ]);
                        }, memberData);
                        Bisnis.Util.Grid.renderRecords('#publishAdsList', records, pageNum);
                    } else {
                        Bisnis.Util.Document.putHtml('#publishAdsList', '<tr><td colspan="10">BELUM ADA DATA</td></tr>');
                    }
                }, function () {
                    Bisnis.Util.Dialog.alert(' PERHATIAN', 'GAGAL MEMUAT EDISI TERBIT');
                }
            );
        };
        // End Edisi Terbit

        // End Klik Perbarui
        Bisnis.Util.Event.bind('click', '#btn-order', function () {
            var thisBtn = this;
            thisBtn.disabled = true;
            thisBtn.innerHTML = '<i class="fa fa-circle-o-notch fa-spin"></i> TUNGGU...';

            var params = Bisnis.Util.Form.serializeArray('#orderForm');
            const selectedTags = document.querySelectorAll('[name="orderTag"] option:checked');
            const values = Array.from(selectedTags).map((el) => el.value);

            params.push({
                name: 'orderTag',
                value: values.toString()
            });

            var fields = [
                'basePrice',
                'taxValue',
                'minDiscountValue',
                'surchargeValue',
                'discountValue',
                'cashBackValue',
                'totalAmount',
            ];

            params = Bisnis.Util.Form.unFormatMoney(fields, params);
            var dateFields = ['bookedAt', 'invoicedAt'];
            params = Bisnis.Util.Form.formatDate(dateFields, params, 'DD/MM/YYYY');

            var id = window.location.pathname.split( '/' )[3];

            Bisnis.Adv.Orders.updateById(id, params,
                function () {
                    thisBtn.innerHTML = 'SIMPAN';
                    Bisnis.Util.Dialog.alert('SUKSES', 'ORDER IKLAN BERHASIL DIPERBARUI', function () {
                        window.location.href = '/advertising/orders';
                    });
                },
                function (response) {
                    if (response.responseJSON) {
                        Bisnis.Util.Grid.validate('orderForm', response.responseJSON.violations);
                    }
                    thisBtn.innerHTML = 'SIMPAN';
                    thisBtn.disabled = false;
                }
            );
        })
        // End Klik Perbarui
    </script>
{% endblock %}

